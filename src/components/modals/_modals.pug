        svg(xmlns='http://www.w3.org/2000/svg' width='32' height='32' fill='none' viewBox='0 0 32 32')
          path(fill='#0E0E0E' d='m5.39 25.2 19.8-19.8 1.41 1.4L6.8 26.6z')
          path(fill='#0E0E0E' d='m6.81 5.4 19.8 19.8-1.41 1.4L5.4 6.8z')
    .modal__body
      .n-galleryProject
        .n-galleryProject__list._title-list 
          - var val = 0;
          while val < 3
            each title in ['Gallery', '12 ', 'Project P371']
              .n-galleryProject__item 
                .n-button1=title
              - val++
        .n-galleryProject__list 
          - var val = 0;
          while val < 10
            a.n-galleryProject__item(data-fancybox="gallery" href="/img/newrubkoff/fancyb-1.jpg") 
              .n-galleryProject__item-bg
                .n-galleryProject__item-bgWrapper
                  img(src="/img/newrubkoff/gallery-small1.jpg" alt="")
            - val++
          - var val = 0;
          while val < 10
            a.n-galleryProject__item(data-fancybox="gallery" href="/img/newrubkoff/gallery-small2.jpg") 
              .n-galleryProject__item
                .n-galleryProject__item-bg
                  .n-galleryProject__item-bgWrapper
                    img(src="/img/newrubkoff/gallery-small2.jpg" alt="")
            - val++

